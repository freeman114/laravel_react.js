        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "popper.js": "^1.12",
        "react": "^16.2.0",
        "react-app-rewired": "^2.1.6",
        "react-dom": "^16.2.0",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.20.1",
        "sass-loader": "^8.0.0"
    },
    "dependencies": {
        "@stripe/react-stripe-js": "^1.1.2",
        "@stripe/stripe-js": "^1.8.0",
        "ag-grid-community": "^23.0.2",
